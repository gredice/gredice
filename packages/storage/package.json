{
    "name": "@gredice/storage",
    "version": "0.0.0",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "license": "MIT",
    "exports": {
        ".": "./src/index.ts"
    },
    "scripts": {
        "clean": "rimraf .turbo dist build out coverage tsconfig.tsbuildinfo",
        "lint": "biome check --write",
        "lint:migrate": "biome migrate --write",
        "db-generate": "drizzle-kit generate",
        "db-push": "tsx --env-file=.env ./src/migrate.ts",
        "dev": "drizzle-kit studio",
        "test": "pnpm run test:node",
        "test:db:start": "bash ./tests/startTestDb.sh",
        "test:db:migrate": "tsx --env-file=.env.test --conditions=react-server ./src/migrate.ts",
        "test:db:stop": "bash ./tests/stopTestDb.sh",
        "test:node": "pnpm run test:db:start && pnpm run test:db:migrate && node --import tsx --test --env-file=.env.test --conditions=react-server ./tests/**/*.node.spec.ts && pnpm run test:db:stop"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.13",
        "@gredice/directory-types": "workspace:*",
        "@gredice/js": "workspace:*",
        "@neondatabase/serverless": "1.0.2",
        "@signalco/js": "0.1.0",
        "@upstash/redis": "1.36.1",
        "dotenv": "17.2.3",
        "drizzle-kit": "0.31.8",
        "drizzle-orm": "0.45.1",
        "openapi-types": "12.1.3",
        "pg": "8.17.2",
        "server-only": "0.0.1",
        "tsx": "4.21.0",
        "typescript": "5.9.3",
        "uuid": "13.0.0"
    }
}
